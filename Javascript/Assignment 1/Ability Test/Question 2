function Max(arr){
    var max= arr[0];
    for(i=1;i<arr.length;i++)
   {
            if(arr[i]>max) 
            {
               max =arr[i];
            }
   }
   return max;
}

  

function min(arr){
    var min= arr[0];
    for(i=1;i<arr.length;i++)
   {
            if(arr[i]<min) 
            {
               min =arr[i];
            }
   }
   return min;
}
var finalResult= Max([1,2,3,5,7,1]);
var finalResult1= min([1,2,3,5,7,1]);

var emptyarr =[];
emptyarr.push(finalResult1,finalResult);
console.log(emptyarr);


        
